{
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": "webpack",
  },
  "extends": "eslint-config-airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    semi: [2, "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "max-len": [1, { "code": 120 }]
  },
}
